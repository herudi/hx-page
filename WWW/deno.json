    "hxp/pages": "./pages.ts",
    "hxp/pages/": "./pages/"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "hxp"
  },
  "deploy": {
    "project": "bff16e7b-3726-4f63-895f-b5d8bf1aa7e1",
    "exclude": [
      "**/node_modules"
    ],
    "include": [],
